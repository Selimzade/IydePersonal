@using IydePersonal.WEB.Models

@model List<EmployeeDetailViewModel>


@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


@foreach (var employees in Model)
{
    <h1>@employees.FirstName</h1> <br />
    <h1>@employees.LastName</h1> <br />
    <h1>@employees.DateOfBirth</h1> <br />
    <h1>@employees.Gender</h1>
    <h1>@employees.PhoneNumber</h1>
    <h1>@employees.FixSalary</h1>
    <h1>@employees.WorkPosition</h1>
    <h3>@employees.StartWork</h3>
    
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    

    <br />
    <br />

}

@Html.ValidationSummary()

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    ;
    <input type="hidden" name="returnUrl" value="@ViewBag.returnUrl" />
    <div class="form-group">
        <label>Имя</label>
        
    </div>
    <div class="form-group">
        <label>Пароль</label>
       
    </div>
    <button class="btn btn-primary" type="submit">Войти</button>
}